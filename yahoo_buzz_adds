WITH adds AS (
	SELECT
		player,
		adds,
		pct_of_total_adds,
		LAG(adds) OVER (
			PARTITION BY player
			ORDER BY time_fetched) AS prev_adds,
		time_fetched
	FROM yahoo_buzz
)
SELECT
	player,
	prev_adds,
	adds,
	pct_of_total_adds,
	ROUND((adds - prev_adds) / adds * 100, 2) AS pct_change_from_prev,
	time_fetched
FROM adds